public class sample3 {
    public static void main(String[] args) {
        int[][]a={{1,2,3,4},
                  {3,5,6,4},
                  {4,3,7,6},
                  {1,6,4,3}
        };
        for(int i=0;i<a.length;i++){
            for(int j=0;j<a.length;j++){
               System.out.print(a[i][j]+"\t");   
            }
        }
          
        }
}      
