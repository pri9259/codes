public class CountCharacter{
    public static int CountOccurrences(String str,char ch) {
        int total=0;
        for(char c:str.tocharArray()){
            if(c==ch)
             total++;}
             return total;
        }
        public static void main(String[] args){
        System.out.println(CountOccurs("sql java html",'a'));
    }
}
