mport java.util.Scanner;public class addwithoutplus {    public static int sub(int a,int b){        for(int i=1;i<=b;i++){            a--;        }        return a;    }    public static void main(String[]args){        Scanner sc=new Scanner(System.in);        System.out.print("Enter first number:");        int x=sc.nextInt();        System.out.print("Enter second number:");        int y=sc.nextInt();        int result=sub(x,y);        System.out.println("Sum without '+' is :" +result);    }}
